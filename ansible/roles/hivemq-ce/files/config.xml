<?xml version="1.0"?>
<hivemq>
  <listeners>
    <tcp-listener>
      <port>1883</port>
      <bind-address>0.0.0.0</bind-address>
    </tcp-listener>
  </listeners>
  <mqtt>
    <session-expiry>
      <max-interval>3600</max-interval>
    </session-expiry>
  </mqtt>
  <security>
    <allow-empty-client-id>
      <enabled>false</enabled>
    </allow-empty-client-id>
  </security>
</hivemq>